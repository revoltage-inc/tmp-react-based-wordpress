{
  "name": "tmp-react-based-wordpress",
  "version": "1.0.0",
  "description": "Template for WordPress theme using React, Vite, TailwindCSS, etc.",
  "author": "@yogarasu",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
